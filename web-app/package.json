{
  "name": "next-payload-stack",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "watch-build": "tsc --watch",
    "watch-test": "vitest --watch",
    "test": "vitest --run",
    "test:report": "vitest --run --reporter=verbose,dot",
    "coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "watch": "concurrently --names 'TS,JEST' -c 'red,blue' \"npm run watch-build\" \"npm run watch-test\"",
    "test:e2e": "playwright test",
    "sentry:release": "sentry-cli releases new $npm_package_version && sentry-cli releases set-commits $npm_package_version --auto && sentry-cli releases finalize $npm_package_version"
  },
  "dependencies": {
    "@sentry/nextjs": "^8.34.0",
    "@stripe/stripe-js": "^4.8.0",
    "axe-playwright": "^2.0.3",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^11.11.9",
    "jest-axe": "^9.0.0",
    "lint-staged": "^15.2.10",
    "lucide-react": "^0.453.0",
    "next": "14.2.15",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "stripe": "^17.2.1",
    "tailwind-merge": "^2.5.4",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@playwright/test": "^1.48.1",
    "@storybook/addon-a11y": "^8.3.6",
    "@storybook/addon-essentials": "^8.3.6",
    "@storybook/addon-interactions": "^8.3.6",
    "@storybook/addon-links": "^8.3.6",
    "@storybook/addon-onboarding": "^8.3.6",
    "@storybook/addon-themes": "^8.3.6",
    "@storybook/blocks": "^8.3.6",
    "@storybook/nextjs": "^8.3.6",
    "@storybook/react": "^8.3.6",
    "@storybook/test": "^8.3.6",
    "@storybook/test-runner": "^0.19.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@types/node": "20.8.3",
    "@types/react": "18.2.25",
    "@types/react-dom": "18.2.11",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/coverage-v8": "^2.1.3",
    "@vitest/ui": "^2.1.3",
    "concurrently": "^9.0.1",
    "eslint": "8.51.0",
    "eslint-config-next": "14.2.15",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-storybook": "^0.9.0",
    "jsdom": "^25.0.1",
    "playwright": "^1.48.1",
    "postcss": "8.4.31",
    "prettier": "^3.3.3",
    "storybook": "^8.3.6",
    "tailwindcss": "3.4.1",
    "typescript": "5.2.2",
    "vercel": "^37.11.0",
    "vitest": "^2.1.3"
  }
}
